        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#ProcessualContext</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">processual_context</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#ProcessualEntity</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">DEFINITION: An occurrent [Occurrent] that exists in time by occurring or happening, has temporal parts and always involves and depends on some entity.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#ProcessualEntity</IRI>
