    <owl:Class rdf:about="http://purl.obolibrary.org/obo/example/course">
        <rdfs:label xml:lang="en">course</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/example/process"/>
                    <owl:Restriction>
