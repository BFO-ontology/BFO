<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
<!ENTITY base "http://www.ifomis.de/bfo/1.0">
<!ENTITY bfo "&base;#">
<!ENTITY snap "&base;/snap#">
<!ENTITY span "&base;/span#">
<!ENTITY dc "http://purl.org/dc/elements/1.1/">
<!ENTITY w3 "http://www.w3.org">
<!ENTITY owl "&w3;/2002/07/owl#">
<!ENTITY rdf "&w3;/1999/02/22-rdf-syntax-ns#">
<!ENTITY rdfs "&w3;/2000/01/rdf-schema#">
<!ENTITY xsd "&w3;/2001/XMLSchema#">
]>

<rdf:RDF xmlns:bfo="&bfo;"
		xmlns:snap="&snap;"
		xmlns:span="&span;"
		xmlns:dc="&dc;"
		xmlns:owl="&owl;"
		xmlns:rdf="&rdf;"
		xmlns:rdfs="&rdfs;"
		xmlns:xsd="&xsd;"
		xml:base="&base;">
	<owl:Ontology rdf:about="">
		<dc:title rdf:datatype="&xsd;string">Basic Formal Ontology (BFO)</dc:title>
		<dc:creator rdf:datatype="&xsd;string">Holger Stenzhorn</dc:creator>
		<dc:contributor rdf:datatype="&xsd;string">Andrew Spear</dc:contributor>
		<dc:publisher>Institute for Formal Ontology and Medical Information Science (IFOMIS)</dc:publisher>
		<dc:source rdf:datatype="&xsd;string">Pierre Grenon and Barry Smith: "SNAP and SPAN: Towards Geospatial Dynamics"</dc:source>
		<dc:source rdf:datatype="&xsd;string">Barry Smith and Pierre Grenon: "The Cornucopia of Formal-Ontological Relations"</dc:source>
		<dc:source rdf:datatype="&xsd;string">Pierre Grenon, Barry Smith and Louis Goldberg: "Biodynamic Ontology: Applying BFO in the Biomedical Domain"</dc:source>
		<dc:date rdf:datatype="&xsd;date">2006-10-14</dc:date>
		<rdfs:comment rdf:datatype="&xsd;string">This is a beta version! Documentation and examples are still incomplete or missing. Comments are welcome!</rdfs:comment>
		<owl:versionInfo rdf:datatype="&xsd;string">1.0 beta</owl:versionInfo>
		<owl:imports rdf:resource="&dc;"/>
	</owl:Ontology>


	<!-- BFO -->


	<owl:Class rdf:about="&bfo;Entity">
		<rdfs:label rdf:datatype="&xsd;string">entity</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:unionOf rdf:parseType="Collection">
			<owl:Class rdf:about="&snap;Continuant"/>
			<owl:Class rdf:about="&span;Occurrent"/>
		</owl:unionOf>
	</owl:Class>


	<!-- SNAP -->


	<owl:Class rdf:about="&snap;AggregateOfSubstances">
		<rdfs:subClassOf rdf:resource="&snap;SubstantialEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">aggregate_of_substances</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a substantial entity that is a mereological sum of separate substances and possesses non-connected boundaries</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a heap of stones, a group of commuters on the subway, a collection of random bacteria, a flock of geese, the patients in a hospital</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;BoundaryOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;FiatPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;StructuralPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;Substance"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;BoundaryOfSubstance">
		<rdfs:subClassOf rdf:resource="&snap;SubstantialEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">boundary_of_substance</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a substantial entity that is a lower dimensional part of a spatial entity, normally a closed two-dimensional surface</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the surface of the skin, the surface of the earth, the surface of the interior of the stomach</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;AggregateOfSubstances"/>
		<owl:disjointWith rdf:resource="&snap;FiatPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;StructuralPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;Substance"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Continuant">
		<rdfs:subClassOf rdf:resource="&bfo;Entity"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&snap;DependentContinuant"/>
					<owl:Class rdf:about="&snap;IndependentContinuant"/>
					<owl:Class rdf:about="&snap;SpatialRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">continuant</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: an entity that exists in full at any time in which it exists at all, persists through time while maintaining its identity and has no temporal parts</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a heart, a person, the color of tomato, the mass of a cloud, a symphony orchestra, the lawn in front of our building</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;Occurrent"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;DependentContinuant">
		<rdfs:subClassOf rdf:resource="&snap;Continuant"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">					
					<owl:Class rdf:about="&snap;Disposition"/>			
					<owl:Class rdf:about="&snap;Function"/>
					<owl:Class rdf:about="&snap;Power"/>
					<owl:Class rdf:about="&snap;Quality"/>					
					<owl:Class rdf:about="&snap;Role"/>					
					<owl:Class rdf:about="&snap;State"/>									
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">dependent_continuant</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a continuant that can exist only if some other entity or entities exist, the existence of which makes its existence possible</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the mass of a cloud, the color of a tomato</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;IndependentContinuant"/>
		<owl:disjointWith rdf:resource="&snap;SpatialRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Disposition">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">disposition</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the disposition of a vegetable to decay</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Function"/>
		<owl:disjointWith rdf:resource="&snap;Power"/>
		<owl:disjointWith rdf:resource="&snap;Quality"/>
		<owl:disjointWith rdf:resource="&snap;Role"/>
		<owl:disjointWith rdf:resource="&snap;State"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;FiatPartOfSubstance">
		<rdfs:subClassOf rdf:resource="&snap;SubstantialEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">fiat_part_of_substance</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a substantial entity that is part of a substance but is not demarcated by any physical discontinuities</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: upper and lower lobes of the left lung, the dorsal and ventral surfaces of the body, the east side of Saarbruecken</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;AggregateOfSubstances"/>
		<owl:disjointWith rdf:resource="&snap;BoundaryOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;StructuralPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;Substance"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Function">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">function</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the function of a canal (i.e. the birth canal) to enable transport</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Disposition"/>
		<owl:disjointWith rdf:resource="&snap;Power"/>
		<owl:disjointWith rdf:resource="&snap;Quality"/>
		<owl:disjointWith rdf:resource="&snap;Role"/>
		<owl:disjointWith rdf:resource="&snap;State"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;IndependentContinuant">
		<rdfs:subClassOf rdf:resource="&snap;Continuant"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&snap;Site"/>
					<owl:Class rdf:about="&snap;SubstantialEntity"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">independent_continuant</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a continuant that can exist and be what it is regardless of what other objects exist</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a heart, a person, a symphony orchestra, the lawn in front of our building</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;DependentContinuant"/>
		<owl:disjointWith rdf:resource="&snap;SpatialRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Line">
		<rdfs:subClassOf rdf:resource="&snap;SpatialRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">line</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a spatial region with one dimension</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the space occupied by the edge of a table, the (idealized) space occupied by a surgical incision</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Point"/>
		<owl:disjointWith rdf:resource="&snap;Surface"/>
		<owl:disjointWith rdf:resource="&snap;Volume"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Point">
		<rdfs:subClassOf rdf:resource="&snap;SpatialRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">point</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a spatial region with no dimensions</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a point</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Line"/>
		<owl:disjointWith rdf:resource="&snap;Surface"/>
		<owl:disjointWith rdf:resource="&snap;Volume"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Power">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">power</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the power of a refrigerator to slow down the decay of vegetables</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Disposition"/>
		<owl:disjointWith rdf:resource="&snap;Function"/>
		<owl:disjointWith rdf:resource="&snap;Quality"/>
		<owl:disjointWith rdf:resource="&snap;Role"/>
		<owl:disjointWith rdf:resource="&snap;State"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Quality">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">quality</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the color of a tomato, the ambient temperature of air, the circumference of a waist, the shape of a nose</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Disposition"/>
		<owl:disjointWith rdf:resource="&snap;Function"/>
		<owl:disjointWith rdf:resource="&snap;Power"/>
		<owl:disjointWith rdf:resource="&snap;Role"/>
		<owl:disjointWith rdf:resource="&snap;State"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Role">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">role</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the role as a student, the role as the captain of a soccer team, the role as the president of a nation</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Disposition"/>
		<owl:disjointWith rdf:resource="&snap;Function"/>
		<owl:disjointWith rdf:resource="&snap;Power"/>
		<owl:disjointWith rdf:resource="&snap;Quality"/>
		<owl:disjointWith rdf:resource="&snap;State"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Site">
		<rdfs:subClassOf rdf:resource="&snap;IndependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">site</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: an independent continuant that is the sum of a substantial entity or entities and a spatial region or spatial regions</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the front lawn of our building, a nasal cavity, a blood vein, an environment, sinuses, canals, ventricles, the lumen of the gastrointestinal tract, the interior of the aorta, the room in which one is located</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;SubstantialEntity"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;SpatialRegion">
		<rdfs:subClassOf rdf:resource="&snap;Continuant"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&snap;Line"/>
					<owl:Class rdf:about="&snap;Point"/>
					<owl:Class rdf:about="&snap;Surface"/>
					<owl:Class rdf:about="&snap;Volume"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">spatial_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a continuant at or in which other continuants can be located</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the space occupied by an appendix, the space that was occupied by an appendix prior to its removal</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;DependentContinuant"/>
		<owl:disjointWith rdf:resource="&snap;IndependentContinuant"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;State">
		<rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
		<rdfs:label rdf:datatype="&xsd;string">state</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Disposition"/>
		<owl:disjointWith rdf:resource="&snap;Function"/>
		<owl:disjointWith rdf:resource="&snap;Power"/>
		<owl:disjointWith rdf:resource="&snap;Quality"/>
		<owl:disjointWith rdf:resource="&snap;Role"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;StructuralPartOfSubstance">
		<rdfs:subClassOf rdf:resource="&snap;SubstantialEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">structural_part_of_substance</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;AggregateOfSubstances"/>
		<owl:disjointWith rdf:resource="&snap;BoundaryOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;FiatPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;Substance"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Substance">
		<rdfs:subClassOf rdf:resource="&snap;SubstantialEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">substance</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a substantial entity that is self-connected and self-contained, possessing an internal unity and instantiating a natural kind, that maintains identity through time while gaining and losing qualities or parts and that exists in its entirety at any moment in which it exists at all</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: an organism, a heart, a symphony orchestra, a chair</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;AggregateOfSubstances"/>
		<owl:disjointWith rdf:resource="&snap;BoundaryOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;FiatPartOfSubstance"/>
		<owl:disjointWith rdf:resource="&snap;StructuralPartOfSubstance"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;SubstantialEntity">
		<rdfs:subClassOf rdf:resource="&snap;IndependentContinuant"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&snap;AggregateOfSubstances"/>
					<owl:Class rdf:about="&snap;BoundaryOfSubstance"/>
					<owl:Class rdf:about="&snap;FiatPartOfSubstance"/>
					<owl:Class rdf:about="&snap;StructuralPartOfSubstance"/>
					<owl:Class rdf:about="&snap;Substance"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">substantial_entity</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: independent continuants that are self-connected and maintain their identity through time while gaining and losing qualities or parts and that exist in their entirety at any moment in which they exist at all</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: an organism, a heart, a symphony orchestra, a chair</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Site"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Surface">
		<rdfs:subClassOf rdf:resource="&snap;SpatialRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">surface</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a spatial region with two dimensions</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the surface of the Atlantic Ocean, the surface of a patients skin, the surface of the interior lining of the stomach</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Line"/>
		<owl:disjointWith rdf:resource="&snap;Point"/>
		<owl:disjointWith rdf:resource="&snap;Volume"/>
	</owl:Class>
	<owl:Class rdf:about="&snap;Volume">
		<rdfs:subClassOf rdf:resource="&snap;SpatialRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">volume</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a spatial region with three dimensions</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a cube, the inside of a skull, the space occupied by a pulmonary artery</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Line"/>
		<owl:disjointWith rdf:resource="&snap;Point"/>
		<owl:disjointWith rdf:resource="&snap;Surface"/>
	</owl:Class>


	<!-- SPAN -->


	<owl:Class rdf:about="&span;AggregateOfProcesses">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">aggregate_of_processes</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a processual entity that is the mereological sum of two or more processes</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;FiatPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;Process"/>
		<owl:disjointWith rdf:resource="&span;Setting"/>
		<owl:disjointWith rdf:resource="&span;StructuralPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;TemporalBoundaryOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;ConnectedSpatiotemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;SpatiotemporalRegion"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;InstantSpatiotemporalRegion"/>
					<owl:Class rdf:about="&span;IntervalSpatiotemporalRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">connected_spatiotemporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a space time region that has temporal and spatial dimensions such that all points within the spatiotemporal region are mediately or immediately connected to all other points within the same space time region</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the spatial and temporal location of an individual organismâ€™s life, the spatial and temporal location of the development of a fetus</rdfs:comment>
	</owl:Class>
	<owl:Class rdf:about="&span;ConnectedTemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;TemporalRegion"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;InstantTemporalRegion"/>
					<owl:Class rdf:about="&span;IntervalTemporalRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">connected_temporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a temporal region every point of which is mediately or immediately connected with every other point of which</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the 1970s years, the time from the beginning to the end of a heart attack, the time taken up by cellular meiosis</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;ScatteredTemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;FiatPartOfProcess">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">fiat_part_of_process</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a processual entity that is part of a process but that does not have bona fide beginnings and endings corresponding to real discontinuities</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: chewing during a meal, the middle part of a rainstorm, the worst part of a heart-attack</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;AggregateOfProcesses"/>
		<owl:disjointWith rdf:resource="&span;Process"/>
		<owl:disjointWith rdf:resource="&span;Setting"/>
		<owl:disjointWith rdf:resource="&span;StructuralPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;TemporalBoundaryOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;InstantSpatiotemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;ConnectedSpatiotemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">instant_spatiotemporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
	</owl:Class>
	<owl:Class rdf:about="&span;InstantTemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;ConnectedTemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">instant_temporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;IntervalTemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;IntervalSpatiotemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;ConnectedSpatiotemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">interval_spatiotemporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
	</owl:Class>
	<owl:Class rdf:about="&span;IntervalTemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;ConnectedTemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">interval_temporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;InstantTemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;Occurrent">
		<rdfs:subClassOf rdf:resource="&bfo;Entity"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;ProcessualEntity"/>
					<owl:Class rdf:about="&span;SpatiotemporalRegion"/>
					<owl:Class rdf:about="&span;TemporalRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">occurrent</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&snap;Continuant"/>
	</owl:Class>
	<owl:Class rdf:about="&span;Process">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">process</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a processual entity that is a maximally connected spatio-temporal whole and has bona fide beginnings and endings corresponding to real discontinuities</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the life of an organism, the process of sleeping</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;AggregateOfProcesses"/>
		<owl:disjointWith rdf:resource="&span;FiatPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;Setting"/>
		<owl:disjointWith rdf:resource="&span;StructuralPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;TemporalBoundaryOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;ProcessualEntity">
		<rdfs:subClassOf rdf:resource="&span;Occurrent"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;AggregateOfProcesses"/>
					<owl:Class rdf:about="&span;FiatPartOfProcess"/>
					<owl:Class rdf:about="&span;Process"/>
					<owl:Class rdf:about="&span;Setting"/>
					<owl:Class rdf:about="&span;StructuralPartOfProcess"/>
					<owl:Class rdf:about="&span;TemporalBoundaryOfProcess"/>						
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">processual_entity</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: an occurrent that exists in time by occurring or happening, has temporal parts and always involves and depends on some SNAP entity</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;TemporalRegion"/>
		<owl:disjointWith rdf:resource="&span;SpatiotemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;ScatteredSpatiotemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;SpatiotemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">scattered_spatiotemporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a space time region that has spatial and temporal dimensions and every spatial and temporal point of which is not connected with every other spatial and temporal point of which</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the space and time occupied by the individual games of the World Cup, the space and time occupied by the individual liaisons in a romantic affair</rdfs:comment>
	</owl:Class>
	<owl:Class rdf:about="&span;ScatteredTemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;TemporalRegion"/>
		<rdfs:label rdf:datatype="&xsd;string">scattered_temporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a temporal region every point of which is not mediately or immediately connected with every other point of which</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the time occupied by the individual games of the World Cup, the time occupied by the individual liaisons in a romantic affair</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;ConnectedTemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;Setting">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">setting</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a processual entity that is the sum of processual entities, spatiotemporal regions and temporal regions</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: a surgical operation as setting for a nosocomical infection, a routine check-up as setting for the finding of a tumor</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;AggregateOfProcesses"/>
		<owl:disjointWith rdf:resource="&span;FiatPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;Process"/>
		<owl:disjointWith rdf:resource="&span;StructuralPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;TemporalBoundaryOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;SpatiotemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;Occurrent"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;ConnectedSpatiotemporalRegion"/>
					<owl:Class rdf:about="&span;ScatteredSpatiotemporalRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">spatiotemporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: an occurrent at or in which processual entities can be located</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the spatiotemporal region occupied by a human life, the spatiotemporal region occupied by the development of a cancer tumor, the spatiotemporal setting occupied by a process of cellular meiosis</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;ProcessualEntity"/>
		<owl:disjointWith rdf:resource="&span;TemporalRegion"/>
	</owl:Class>
	<owl:Class rdf:about="&span;StructuralPartOfProcess">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">structural_part_of_process</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition:</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples:</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;AggregateOfProcesses"/>
		<owl:disjointWith rdf:resource="&span;FiatPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;Process"/>
		<owl:disjointWith rdf:resource="&span;Setting"/>
		<owl:disjointWith rdf:resource="&span;TemporalBoundaryOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;TemporalBoundaryOfProcess">
		<rdfs:subClassOf rdf:resource="&span;ProcessualEntity"/>
		<rdfs:label rdf:datatype="&xsd;string">temporal_boundary_of_process</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: a processual entity that is the fiat or bona fide temporal boundary of a process</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the forming of a synapse, the onset of REM sleep, the detaching of a finger in an industrial accident</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;AggregateOfProcesses"/>
		<owl:disjointWith rdf:resource="&span;FiatPartOfProcess"/>
		<owl:disjointWith rdf:resource="&span;Process"/>
		<owl:disjointWith rdf:resource="&span;Setting"/>
		<owl:disjointWith rdf:resource="&span;StructuralPartOfProcess"/>
	</owl:Class>
	<owl:Class rdf:about="&span;TemporalRegion">
		<rdfs:subClassOf rdf:resource="&span;Occurrent"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="&span;ConnectedTemporalRegion"/>
					<owl:Class rdf:about="&span;ScatteredTemporalRegion"/>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<rdfs:label rdf:datatype="&xsd;string">temporal_region</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Definition: an occurrent that is part of time</rdfs:comment>
		<rdfs:comment rdf:datatype="&xsd;string">Examples: the time it takes to run a marathon, the duration of a surgical procedure, the moment of death</rdfs:comment>
		<owl:disjointWith rdf:resource="&span;ProcessualEntity"/>
		<owl:disjointWith rdf:resource="&span;SpatiotemporalRegion"/>
	</owl:Class>
</rdf:RDF>
