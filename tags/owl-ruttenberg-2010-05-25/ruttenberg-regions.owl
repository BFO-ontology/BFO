        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
        <obo:IAO_0000115 xml:lang="en">A spatial region  with one dimension.</obo:IAO_0000115>
        <obo:IAO_0000112 xml:lang="en">an edge of a cube-shaped part of space</obo:IAO_0000112>
        <obo:IAO_0000111 xml:lang="en">one dimensional region</obo:IAO_0000111>
        <obo:IAO_0000112 xml:lang="en">the part of space that is a line stretching from one end of absolute space to the other</obo:IAO_0000112>
    </owl:Class>
    


