        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
        <obo:IAO_0000115 xml:lang="en">A spatial region  with no dimensions.</obo:IAO_0000115>
        <obo:IAO_0000112 xml:lang="en">a point</obo:IAO_0000112>
        <obo:IAO_0000111 xml:lang="en">zero dimensional region</obo:IAO_0000111>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000026 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000026">
