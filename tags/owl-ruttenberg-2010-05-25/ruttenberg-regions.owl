    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000036 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000036">
        <rdfs:label xml:lang="en">spatiotemporal interval</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000013"/>
        <obo:IAO_0000115 xml:lang="en">A connected space time region  that endures for more than a single moment of time.</obo:IAO_0000115>
        <obo:IAO_0000111 xml:lang="en">spatiotemporal interval</obo:IAO_0000111>
        <obo:IAO_0000112 xml:lang="en">the space time region occupied by a process or by a fiat processual part</obo:IAO_0000112>
    </owl:Class>
