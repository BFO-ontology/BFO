        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000003"/>
        <obo:IAO_0000115 xml:lang="en">A processual entity  that is the fiat or bona fide instantaneous temporal process boundary.</obo:IAO_0000115>
        <obo:IAO_0000112 xml:lang="en">birth</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">death</obo:IAO_0000112>
